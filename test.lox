fun iter(a) {
    if (a == 0) {
        return 0;
    } 
    return a + iter(a - 1);
}

fun factorial(n) {
    if (n == 0) {
        return 1;
    }
    return n * factorial(n - 1);
}

fun fib(n) {
    if (n == 0 or n == 1) {
        return 1;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}

